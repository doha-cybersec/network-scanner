{
   "author": "Opera Norway AS",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "index.js" ],
      "matches": [ "chrome://startpage/*" ],
      "run_at": "document_end"
   } ],
   "description": "Startpage Special Extension",
   "developer": {
      "name": "Opera Software"
   },
   "homepage_url": "https://opera.com",
   "key": "",
   "manifest_version": 3,
   "name": "opera-startpage-special",
   "permissions": [ "browserSidebarPrivate", "feedbackPopupPrivate", "speeddialPrivate", "statsPrivate", "storage", "tabs" ],
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "0.1.42",
   "web_accessible_resources": [ {
      "matches": [ "chrome://startpage/*" ],
      "resources": [ "fonts/*.ttf" ]
   }, {
      "matches": [ "chrome://startpage/*" ],
      "resources": [ "imgs/*" ]
   } ]
}
